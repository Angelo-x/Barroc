<Window x:Class="barrocFactuur.ShowLeases"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:barrocFactuur"
        mc:Ignorable="d"
        Title="ShowLeases" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="invcoicesList" Margin="10,10,0,0" Loaded="invcoicesList_Loaded"  ItemsSource="{Binding}" ColumnWidth="*" HorizontalAlignment="Left" Width="774" Height="400" VerticalAlignment="Top"/>
    </Grid>
</Window>
